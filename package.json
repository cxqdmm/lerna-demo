{
  "name": "lerna-demo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "node inquiry-dev.js",
    "dev": "lerna run dev --parallel",
    "build": "lerna run build",
    "lint": "lerna run lint",
    "lint:fix": "lerna run lint:fix",
    "format": "prettier --write \"packages/*/src/**/*.{ts,tsx,vue,js,jsx}\"",
    "type-check": "lerna run type-check",
    "vue-pc": "lerna run dev --scope=vue-pc",
    "vue-mobile": "lerna run dev --scope=vue-mobile",
    "doc": "lerna run dev --scope=doc"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@bundled-es-modules/axios": "^0.27.2",
    "@giom/shared": "*",
    "@types/axios": "^0.14.4",
    "@types/qs": "^6.14.0",
    "ant-design-vue": "^4.2.6",
    "autoprefixer": "^10.4.21",
    "normalize.css": "^8.0.1",
    "pinia": "^3.0.3",
    "qs": "^6.14.0",
    "vant": "^4.8.0",
    "vue": "^3.3.0",
    "vue-router": "^4.5.1",
    "vue-scrollto": "^2.20.0",
    "web-vitals": "^5.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
