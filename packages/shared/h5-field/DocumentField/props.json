{
  "componentName": "DocumentField",
  "componentTitle": "证件输入框",
  "importDesc": "import { DocumentField } from '@shared/h5-field/DocumentField'",
  "description": "专用于证件信息输入的表单组件，支持身份证、护照、港澳台通行证等多种证件类型的选择和输入验证",
  "props": [
    {
      "name": "modelValue",
      "title": "绑定值",
      "type": "object",
      "required": false,
      "description": "证件信息对象，包含type和value字段",
      "default": "{\"type\": \"idcard\", \"value\": \"\"}",
      "control": "input"
    },
    {
      "name": "label",
      "title": "标签文本",
      "type": "string",
      "required": false,
      "description": "输入框的标签",
      "default": "证件信息",
      "control": "input"
    },
    {
      "name": "placeholder",
      "title": "占位符",
      "type": "string",
      "required": false,
      "description": "输入框占位符文本，为空时使用证件类型默认占位符",
      "default": "",
      "control": "input"
    },
    {
      "name": "required",
      "title": "是否必填",
      "type": "boolean",
      "required": false,
      "description": "是否为必填项",
      "default": false,
      "control": "switch",
      "options": [true, false]
    },
    {
      "name": "readonly",
      "title": "是否只读",
      "type": "boolean",
      "required": false,
      "description": "是否为只读状态",
      "default": false,
      "control": "switch",
      "options": [true, false]
    },
    {
      "name": "disabled",
      "title": "是否禁用",
      "type": "boolean",
      "required": false,
      "description": "是否禁用输入框",
      "default": false,
      "control": "switch",
      "options": [true, false]
    },
    {
      "name": "clearable",
      "title": "是否可清空",
      "type": "boolean",
      "required": false,
      "description": "是否显示清空按钮",
      "default": true,
      "control": "switch",
      "options": [true, false]
    },
    {
      "name": "name",
      "title": "表单项名称",
      "type": "string",
      "required": false,
      "description": "表单项的name属性",
      "default": "document",
      "control": "input"
    },
    {
      "name": "enableBuiltInValidation",
      "title": "启用内置验证",
      "type": "boolean",
      "required": false,
      "description": "是否启用内置的证件格式验证",
      "default": true,
      "control": "switch",
      "options": [true, false]
    },
    {
      "name": "trigger",
      "title": "验证触发时机",
      "type": "string",
      "required": false,
      "description": "验证触发的时机",
      "default": "onBlur",
      "control": "radio",
      "options": [
        { "label": "输入时", "value": "onChange" },
        { "label": "失焦时", "value": "onBlur" }
      ]
    },
    {
      "name": "supportedTypes",
      "title": "支持的证件类型",
      "type": "array",
      "required": false,
      "description": "支持选择的证件类型列表",
      "default": "[\"idcard\", \"passport\", \"hkmacaotaiwan\"]",
      "control": "checkbox",
      "options": [
        { "label": "身份证", "value": "idcard" },
        { "label": "护照", "value": "passport" },
        { "label": "港澳台通行证", "value": "hkmacaotaiwan" }
      ]
    }
  ],
  "events": [
    {
      "name": "update:modelValue",
      "description": "值更新时触发",
      "params": "value: DocumentValue"
    },
    {
      "name": "input",
      "description": "输入时触发",
      "params": "value: DocumentValue"
    },
    {
      "name": "blur",
      "description": "失焦时触发",
      "params": "event: Event"
    },
    {
      "name": "focus",
      "description": "聚焦时触发",
      "params": "event: Event"
    },
    {
      "name": "document-parsed",
      "description": "证件信息解析完成时触发",
      "params": "info: DocumentInfo"
    },
    {
      "name": "type-change",
      "description": "证件类型变化时触发",
      "params": "type: DocumentType"
    }
  ],
  "methods": [
    {
      "name": "getDocumentInfo",
      "description": "获取当前证件信息",
      "returns": "DocumentInfo"
    }
  ],
  "slots": []
}